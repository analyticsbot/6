\par \fs28 \b \i0 \ul0 \cf17 \pard \ql \li0\ri0 \sb0 \fi0 NIKE\cf  RIPS INTO WAL-MART FOR SELLING CHEAP KNOCK-OFFS\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 DEVLIN BARRETT
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 All Editions; Pg. 050
