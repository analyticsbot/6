\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 Woods's Golf Balls Roll Into Suit\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 SPORTS; Pg. D02
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 LENGTH: \b0 \i0 \ul0 835 words
