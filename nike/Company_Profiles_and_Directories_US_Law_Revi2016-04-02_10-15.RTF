\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 WOODS, \cf17 NIKE\cf  SIGN DEAL ENDORSEMENT CONTRACT WORTH $100 MILLION\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 MERCURY NEWS WIRE SERVICES
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 SPORTS; Pg. 3D
\par \pard 
