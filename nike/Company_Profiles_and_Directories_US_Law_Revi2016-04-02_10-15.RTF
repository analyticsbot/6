\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 CARTER, \cf17 NIKE\cf  REPORTED NEAR SIGNING A WHOPPING SHOE DEAL\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 SPORTS
