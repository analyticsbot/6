\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SAG BACKERS BLAST \cf17 NIKE\cf \fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 By MICHAEL O'KEEFFE DAILY NEWS SPORTS WRITER
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 SPORTS; Pg. 55
