\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 THOSE 'LOW-PAYING' \cf17 NIKE\cf  JOBS ARE QUITE POPULAR\fs20 \b0 \i0 \ul0 
\par \pard 
