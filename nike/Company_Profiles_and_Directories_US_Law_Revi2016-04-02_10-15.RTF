\par \fs28 \b \i0 \ul0 \cf17 \pard \ql \li0\ri0 \sb0 \fi0 NIKE\cf  BAG DUVAL WITH GBP 16M FIVE-YEAR DEAL\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 MARTIN HARDY
\par \pard 
