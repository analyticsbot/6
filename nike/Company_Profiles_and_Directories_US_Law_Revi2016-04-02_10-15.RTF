\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 GLOBAL PROBLEMS DEMAND GROUP SOLUTIONS FIND COMMON GROUND\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 THOMAS L. FRIEDMAN
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 EDITORIAL; Pg. 8B
