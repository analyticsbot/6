\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 Price walks away from \cf17 Nike\cf \fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 Calgary Herald
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 Golf Weekly; D9
\par \pard 
