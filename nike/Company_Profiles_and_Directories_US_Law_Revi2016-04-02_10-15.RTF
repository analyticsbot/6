\par \fs28 \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 VANDERVEER AT HOME IN \cf17 NIKE'S\cf  BRACKETVILLE\fs20 \b0 \i0 \ul0 
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 BYLINE: \b0 \i0 \ul0 SHELDON SPENCER, Mercury News Staff Writer
\par \pard 
\par \b \i0 \ul0 \pard \ql \li0\ri0 \sb0 \fi0 SECTION: \b0 \i0 \ul0 SPORTS; Pg. 6D
